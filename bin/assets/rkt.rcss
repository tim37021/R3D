/*
* Default styles for all the basic elements.
*/

div
{
	display: block;
}

bl
{
	display: inline-block;
	width: 5;
}

p
{
	display: block;
}

hr
{
	display: block;
}

h1
{
	display: block;
}

em
{
	font-style: italic;
}

strong
{
	font-weight: bold;
}

datagrid
{
